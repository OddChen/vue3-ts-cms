{
  "name": "vue3-ts-cms",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "prettier --check .",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "commit": "cz"
